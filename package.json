{
  "name": "antseller",
  "version": "1.0.0",
  "description": "MERN e-commerce de Fourmis",
  "main": "server.js",
  "type": "module",
  "proxy": 8080,
  "scripts": {
    "start": "node backend/server.js",
    "serve": "nodemon backend/server.js",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seeder",
    "data:destroy": "node backend/seeder -d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Papoel/antseller.git"
  },
  "author": "Pascal Briffard",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Papoel/antseller/issues"
  },
  "homepage": "https://github.com/Papoel/antseller#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-async-errors": "^3.1.1",
    "express-async-handler": "^1.2.0",
    "mongoose": "^6.2.7"
  },
  "devDependencies": {
    "concurrently": "^7.0.0",
    "nodemon": "^2.0.15"
  }
}
